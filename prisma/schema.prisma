// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Service {
  id   String  @id @default(uuid())
  name String   
  price Int
  category String
  images String
  district String
  location String
  description String
  channel String
  hdChannel String
  connectionCost Int
  status String

  @@map("services")
}
